{
	"admin/services/autologin": {
		"title": "Auto Login",
		"order": "60",
		"action": {
			"type": "alias",
			"path": "admin/services/autologin/status"
		},
		"depends": {
			"acl": [ "luci-app-autologin" ],
			"fs": {
				"/usr/sbin/autologin": "executable",
				"/etc/init.d/autologin": "executable"
			},
			"uci": { "autologin": true }
		}
	},

	"admin/services/autologin/status": {
		"title": "Status",
		"order": "1",
		"action": {
			"type": "view",
			"path": "autologin/status"
		}
	},

	"admin/services/autologin/config": {
		"title": "Configuration",
		"order": "2",
		"action": {
			"type": "view",
			"path": "autologin/config"
		}
	}
}
